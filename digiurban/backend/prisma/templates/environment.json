  {
    "code": "ENV_COMPLAINT_004",
    "name": "Denúncia Ambiental",
    "category": "Meio Ambiente",
    "description": "Reportar crime ou infração ambiental",
    "icon": "AlertTriangle",
    "defaultFields": [
      {
        "id": "isAnonymous",
        "label": "Denúncia Anônima",
        "type": "checkbox",
        "required": false,
        "default": false
      },
      {
        "id": "complainantName",
        "label": "Seu Nome",
        "type": "text",
        "required": false,
        "conditionalDisplay": { "field": "isAnonymous", "value": false }
      },
      {
        "id": "complainantPhone",
        "label": "Seu Telefone",
        "type": "phone",
        "required": false,
        "conditionalDisplay": { "field": "isAnonymous", "value": false }
      },
      {
        "id": "complainantEmail",
        "label": "Seu E-mail",
        "type": "email",
        "required": false,
        "conditionalDisplay": { "field": "isAnonymous", "value": false }
      },
      {
        "id": "complaintType",
        "label": "Tipo de Denúncia",
        "type": "select",
        "required": true,
        "options": [
          "Poluição de rio/córrego",
          "Desmatamento ilegal",
          "Maus-tratos a animais",
          "Descarte irregular de lixo",
          "Poluição sonora",
          "Queimadas irregulares",
          "Outro"
        ]
      },
      {
        "id": "description",
        "label": "Descrição da Denúncia",
        "type": "textarea",
        "required": true,
        "placeholder": "Descreva detalhadamente o que está ocorrendo"
      },
      {
        "id": "location",
        "label": "Local da Ocorrência",
        "type": "text",
        "required": true
      },
      {
        "id": "coordinates",
        "label": "Localização GPS",
        "type": "map",
        "required": false
      },
      {
        "id": "photos",
        "label": "Fotos/Evidências",
        "type": "file-multiple",
        "accept": "image/*,video/*",
        "maxFiles": 10,
        "required": false
      },
      {
        "id": "priority",
        "label": "Urgência",
        "type": "select",
        "required": false,
        "options": ["normal", "high", "urgent"],
        "default": "normal"
      }
    ],
    "requiredDocs": [
      "Fotos/vídeos (recomendado)"
    ],
    "estimatedTime": "Imediato",
    "moduleType": "environment",
    "moduleEntity": "EnvironmentalComplaint",
